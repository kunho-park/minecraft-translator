  "formatting_rules": [
    {
      "rule_name": "Placeholder Particle Usage",
      "description": "When a placeholder (%1$s, %2$s, etc.) represents a noun, attach the appropriate Korean subject particle ‘이(가)’ after it. For a second placeholder that is a target, attach ‘에게’. No particles are used for UI strings.",
      "examples": [
        "%1$s이(가) 마법에 의해 죽었습니다",
        "%1$s이(가) %2$s에게 죽었습니다"
      ]
    },
    {
      "rule_name": "Ellipsis Preservation",
      "description": "Ellipsis ‘…’ should be preserved after Korean translations of UI strings.",
      "examples": [
        "설정..."
      ]
    },
    {
      "rule_name": "Transliteration vs Translation",
      "description": "Proper nouns and materials are transliterated (e.g., ‘네더’, ‘레드스톤’), while item types, UI words, and effects are translated into Korean.",
      "examples": [
        "네더라이트 주괴",
        "마법 부여대",
        "날카로움"
      ]
    },
    {
      "rule_name": "Spacing Between Transliteration and Korean Words",
      "description": "When combining transliterated proper nouns with Korean words, use a space between them (e.g., ‘엔더 드래곤’).",
      "examples": [
        "엔더 드래곤",
        "엔더 진주"
      ]
    },
    {
      "rule_name": "Capitalization and Particle Placement",
      "description": "Korean translations are written in lower‑case unless they are part of a proper noun. Use ‘이(가)’ after placeholders and ‘에게’ after the second placeholder; UI words receive no particles.",
      "examples": [
        "완료",
        "게임 종료",
        "%1$s이(가) 마법에 의해 죽었습니다"
      ]
    }
  ]
}